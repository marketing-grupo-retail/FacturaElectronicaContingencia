    <wsdd:parameter name="wsdlTargetNamespace" value="http://servicio.fe.comfandi.grpretail.com"/>
    <wsdd:parameter name="wsdlServiceElement" value="PuenteFEService"/>
    <wsdd:parameter name="schemaQualified" value="http://response.fe.comfandi.grpretail.com,http://servicio.fe.comfandi.grpretail.com"/>
    <wsdd:parameter name="wsdlServicePort" value="PuenteFE"/>
    <wsdd:parameter name="className" value="com.grpretail.comfandi.fe.servicio.PuenteFESoapBindingImpl"/>
    <wsdd:parameter name="wsdlPortType" value="PuenteFE"/>
    <wsdd:parameter name="typeMappingVersion" value="1.2"/>
    <wsdd:parameter name="allowedMethods" value="autenticacion cargaFactura"/>
    <wsdd:typeMapping qname="response.fe.comfandi.grpretail.com:PuenteFEResponse"
        type="java:com.grpretail.comfandi.fe.response.PuenteFEResponse" encodingStyle=""
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory" deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"/>
    <wsdd:typeMapping qname="response.fe.comfandi.grpretail.com:ExceptionDTO" type="java:com.grpretail.comfandi.fe.response.ExceptionDTO"
