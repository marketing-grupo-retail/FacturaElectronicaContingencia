    <wsdd:operation name="cargaFactura" qname="servicio.fe.comfandi.grpretail.com:cargaFactura"
        returnQName="servicio.fe.comfandi.grpretail.com:cargaFacturaReturn" returnType="response.fe.comfandi.grpretail.com:PuenteFEResponse"
        soapAction="">
      <wsdd:parameter qname="servicio.fe.comfandi.grpretail.com:pPointSale" type="XMLSchema:string"/>
      <wsdd:parameter qname="servicio.fe.comfandi.grpretail.com:pAmount" type="XMLSchema:long"/>
