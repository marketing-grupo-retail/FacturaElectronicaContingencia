    <wsdd:operation name="autenticacion" qname="servicio.fe.comfandi.grpretail.com:autenticacion"
        returnQName="servicio.fe.comfandi.grpretail.com:autenticacionReturn" returnType="response.fe.comfandi.grpretail.com:PuenteFEResponse"
